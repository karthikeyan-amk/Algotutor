// https://leetcode.com/problems/running-sum-of-1d-array/submissions/1011720799/

class Solution {
    public int[] runningSum(int[] nums) {
        int[] res = new int[nums.length];
        int sum =0;
        for(int i=0;i<nums.length;i++){
            sum = sum + nums[i];
            res[i] = sum;
        }
        return res;
    }
}